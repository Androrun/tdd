fun main() {
   testSum()
}

fun testSum() {
    
    val num1 = 2
    val num2 = 3
    val expectedSum = 5

    
    val result = sum(num1, num2)

    
    if (result == expectedSum) {
        println("Test Passed: Suma correcta")
    } else {
        println("Test Failed: Suma incorrecta")
    }
}

fun sum(a: Int, b: Int): Int {
    return a + b
}
